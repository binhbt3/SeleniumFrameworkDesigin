<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite parallel="tests" name="Suite">
<groups>
<run>
<include name="ErrorHandling"/>
</run>
</groups>
<listeners>
<listener class-name="BinhBT3.Selenium.TestComponents.Listeners"/>
</listeners>
  <test thread-count="5" name="Submit Order Test">
    <classes>
      <class name="BinhBT3.Selenium.tests.StandAloneTest"/>
      <class name="BinhBT3.Selenium.tests.SubmitOrderTest"/>
    </classes>
    
  </test> <!-- Test -->
  <test thread-count="5" name="Error Validations">
    <classes>
      <class name="BinhBT3.Selenium.tests.ErrorValidationsTest"/>
    </classes>
  </test> <!-- Test -->
</suite> <!-- Suite -->
